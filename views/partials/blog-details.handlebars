{{!-- <h1 id='dashboard-welcome'>Welcome, {{user.username}}!</h1> --}}
<section class="container-fluid mt-5 blog-container">
    <header class='blog-header'>
        <a href="/blog/{{id}}">
            <h2>{{title}}</h2>
        </a>
        <span>posted by: {{user.username}} on: {{format_date createdAt}}</span>
    </header>
    <p>{{content}}</p>
</section>